<div class="ef-field" data-type="submit">
    {% include "./partials/description.njk" %}
    <button
        {% for key, value in attributes %}
        {% if not key == "id" %}{{ key }}="{{ value }}" {% endif %}
        {% endfor %}
        class="{{ html.className | join(' ') }}"
        >
        {{ html.label }}
    </button>
</div>